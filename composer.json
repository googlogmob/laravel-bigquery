{
  "name": "googlogmob/laravel-bigquery",
  "type": "library",
  "description": "Wrapper for Google BigQueryClient",
  "keywords": [
    "googlogmob",
    "laravel-bigquery"
  ],
  "homepage": "https://github.com/googlogmob/laravel-bigquery",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Kravchenko",
      "email": "googlogmob@gmail.com",
      "homepage": "https://github.com/smartmetrika",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.4",
    "psr/cache": "^1.0 | ^2.0 | ^3.0",
    "google/cloud-bigquery": "^1.0.0",
    "illuminate/support": "^10.0 | ^11.0 | ^12.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "rector/rector": "^2.1",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "googlogmob\\BigQuery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "googlogmob\\BigQuery\\": "tests"
    }
  },
  "scripts": {
    "code-style": "PHP_CS_FIXER_IGNORE_ENV=1 php ./vendor/bin/php-cs-fixer fix --allow-risky=yes --diff --config=.php_cs.php",
    "code-style:dry": "PHP_CS_FIXER_IGNORE_ENV=1 php ./vendor/bin/php-cs-fixer fix --allow-risky=yes --verbose --show-progress=dots --dry-run --config=.php_cs.php",

    "rector": "./vendor/bin/rector process --clear-cache",
    "rector:dry": "./vendor/bin/rector process --clear-cache --dry-run",

    "analyse": "./vendor/bin/phpstan analyse src --memory-limit=-1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "googlogmob\\BigQuery\\BigQueryServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
